{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto",
        "plugins": [
          "prettier-plugin-tailwindcss"
        ]
      }
    ],
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}